extends layout

block content
  div#userid.hidden #{userid}
  div#twitterId.hidden #{twitterId}
  div#main
    h1 Canned
    div#controls
      a(href='reset') reset
    div#chat.scroll-pane
      div.message.auto Welcome
    div#compose-others
    div#compose
      input(type='text', id="compose-text")
      input(type='submit', id="compose-submit")
    
